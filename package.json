{
  "type": "module",
  "name": "@dependable/cache",
  "version": "0.12.0",
  "description": "A reactive cache for @dependable/state",
  "author": "Sune Simonsen",
  "license": "MIT",
  "module": "./dist/dependable-cache.esm.js",
  "types": "./dist/Cache.d.ts",
  "exports": {
    ".": {
      "types": "./dist/Cache.d.ts",
      "import": "./dist/dependable-cache.esm.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "dependable",
    "observable",
    "state",
    "cache"
  ],
  "scripts": {
    "api-docs": "typedoc --out api-docs --hideGenerator src/Cache.ts",
    "build": "rm -rf dist && tsc && rollup -c rollup.config.js",
    "coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "format": "prettier --write '**/*.{ts,js,mjs,md,json}'",
    "lint": "eslint '**/*.{ts,js,mjs}' && prettier --check '**/*.{ts,js,mjs,md,json}'",
    "prepublishOnly": "npm run build",
    "publish-docs": "npm run api-docs && surge api-docs dependable-cache-api.surge.sh",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "check:ts": "tsc --noEmit"
  },
  "bugs": "https://github.com/sunesimonsen/dependable-cache/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunesimonsen/dependable-cache"
  },
  "dependencies": {
    "@dependable/state": ">= 0.22"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "fake-promise": "^2.5.44",
    "jest": "^30.2.0",
    "prettier": "^3.0.3",
    "rollup": "^2.27.1",
    "rollup-plugin-terser": "^7.0.2",
    "surge": "^0.23.1",
    "terser": "^5.14.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.26.0",
    "typescript": "^5.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  }
}
